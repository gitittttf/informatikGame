{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "mvnw-compile",
            "type": "shell",
            "command": "${workspaceFolder}/mvnw.cmd",
            "args": [
                "clean",
                "compile"
            ],
            "group": "build",
            "presentation": {
                "echo": true,
                "reveal": "silent",
                "focus": false,
                "panel": "shared"
            },
            "problemMatcher": []
